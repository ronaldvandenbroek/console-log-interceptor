{
  "name": "log-monitor",
  "version": "0.1.0",
  "description": "Server side code to monitor console.log intercepts",
  "main": "index.ts",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "buildstart": "tsc && node dist/index.js",
    "changelint": "npx eslint --init",
    "runlint": "eslint src --ext ts",
    "runlintfix": "eslint src --ext ts --fix"
  },
  "author": "Ronald van den Broek",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/ronaldvandenbroek/console-log-interceptor.git"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "husky": "^4.2.3",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint src --ext ts",
      "pre-push": "eslint src --ext ts"
    }
  }
}
