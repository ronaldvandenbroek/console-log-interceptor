{
  "name": "log-monitor",
  "version": "0.0.0-development",
  "description": "Server side code to monitor console.log intercepts",
  "main": "index.ts",
  "author": "Ronald van den Broek",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ronaldvandenbroek/console-log-interceptor.git"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "buildstart": "npm run build && npm run start",
    "changelint": "npx eslint --init",
    "test:lint": "eslint src --ext ts",
    "test:lintfix": "eslint src --ext ts --fix",
    "test:unit": "mocha",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:lintfix && npm run test:unit",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-push": "npm run test:lintfix && npm run test:unit"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.6",
    "@types/express": "4.17.5",
    "@types/mocha": "7.0.2",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "assert": "2.0.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "6.8.0",
    "eslint-config-google": "0.14.0",
    "husky": "4.2.3",
    "mocha": "7.1.1",
    "semantic-release": "17.0.4",
    "ts-node": "8.8.2",
    "typescript": "3.8.3"
  }
}
